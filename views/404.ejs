    <div class="container">
      <div class="row justify-content-md-center">
        <div class="col-md-8">
          <div class="card">
            <div class="card-header card-title">
              <strong>404- Not Found</strong>
            </div>           
            <div class="card-body">
                <% if(message){%>
                    <h1><%= message %></h1>
                    <%}else{%>
                    <h1>Internal server error</h1>
                   <% } %>

                
            </div>
          </div>
        </div>
      </div>
    </div>
  </main>
</body>
</html>